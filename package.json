{
  "name": "inboundcustomerphonecalls",
  "version": "1.0.0",
  "description": "Maintain a queue of the inbound customer phone calls",
  "main": "app.js",
  "scripts": {
    "start": "cross-env babel-node app.js",
    "build": "babel app.js --out-dir build",
    "dev:start": "cross-env DEBUG=app nodemon --exec babel-node app.js",
    "format": "prettier --write \"./**/*.js\"",
    "test": "jest --silent true --runInBand",
    "test:debug": "jest --debug true --runInBand"
  },
  "keywords": [
    "queue",
    "websocket"
  ],
  "author": "Lincy Gonzalez Rojas",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jest": "^27.3.1",
    "ws": "^8.2.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
  }
}
